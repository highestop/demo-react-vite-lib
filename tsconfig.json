{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "types": ["vite/client", "node"],
    "strict": true,
    "moduleResolution": "Node",
    "skipLibCheck": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true,
    "importsNotUsedAsValues": "error"
  },
  "exclude": ["node_modules", "dist"]
}
